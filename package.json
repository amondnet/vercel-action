{
  "name": "vercel-action",
  "version": "41.1.4",
  "private": true,
  "author": {
    "name": "Minsu Lee",
    "email": "amond@amond.net",
    "url": "https://amond.dev"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/amondnet/vercel-action"
  },
  "keywords": [
    "GitHub",
    "Actions",
    "Vercel",
    "Zeit",
    "Now"
  ],
  "main": "dist/index.js",
  "engines": {
    "node": "v20"
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "start": "node ./dist/src/index.js",
    "package": "npm run build && ncc build dist/src/index.js -o dist --minify --no-cache --no-source-map-register",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "format": "eslint --fix .",
    "all": "npm run format && npm run lint && npm run build && npm run package && npm test"
  },
  "dependencies": {
    "@actions/core": "^1.11.1",
    "@actions/exec": "^1.1.1",
    "@actions/github": "^2.1.1",
    "@octokit/webhooks": "^14.1.1",
    "axios": "^1.10.0",
    "common-tags": "^1.8.2",
    "vercel": "41.1.4"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^5.2.1",
    "@types/common-tags": "^1.8.4",
    "@types/node": "^24.0.13",
    "@vercel/ncc": "^0.36.0",
    "@vitest/ui": "^3.2.4",
    "eslint": "^9.0.0",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  }
}
